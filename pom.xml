    <!-- kie -->
    <dependency>
      <groupId>org.kie</groupId>
      <artifactId>kie-spring</artifactId>
      <version>6.4.0.Final</version>
      <scope>compile</scope>
      <exclusions>
        <exclusion>
          <artifactId>spring-context</artifactId>
